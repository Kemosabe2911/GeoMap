<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Image upload</title>
    <!--Materialize CSS-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
</head>
<body>
    <div class="container">
        <h1>File upload</h1>
        <%= typeof msg !='undefined'? msg : '' %>
        <form action="/uploads" method="POST" enctype="multipart/form-data">
            <div class="file-field input-field">
              <div class="btn grey">
                <span>File</span>
                <input name="pImage" type="file">
              </div>
              <div class="file-path-wrapper">
                <input class="file-path validate" type="text">
              </div>
            </div>
            <button type="submit" class="btn">Submit</button>
          </form>
          <br>
          <!--<img src="/uploads/pImage-1602856106908.png" alt="Image Not availabale" class="responsive-image">-->
          <ul>
          <% imgs.forEach(function(img){ %>
            <li><img src="<%= typeof img.img !='undefined'? `/uploads/${img.img}` : 'Not displaying' %>" alt="Image Not availabale" class="responsive-image"></li>
          <%}); %>
          </ul>
          <!--<img src="<%= typeof img !='undefined'? img : 'Not displaying' %>" alt="Image Not availabale" class="responsive-image">-->
    </div>


    <!--Jquery CDN-->
    <script
			  src="https://code.jquery.com/jquery-3.5.1.min.js"
			  integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
			  crossorigin="anonymous"></script>
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
            
</body>
</html>